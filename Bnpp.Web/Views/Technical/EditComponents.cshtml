<script type="text/javascript">
	$("#TimesOfHeating").kendoDateTimePicker(
		{
			format: "yyyy/MM/dd hh:mm:ss"
		});
	function AjaxdocumentFormSubmit() {
		//Set the URL.
		var url = $("#DesignDocumentForm").attr("action");
		//Add the Field values to FormData object.
		var formData = new FormData();
		formData.append("Item", $("#Item").val());
		formData.append("Designation", $("#Designation").val());
		formData.append("Serial", $("#Serial").val());
		formData.append("Diameter", $("#Diameter").val());
		formData.append("Thickness", $("#Thickness").val());
		formData.append("Length", $("#Length").val());
		formData.append("MaterialGrade", $("#MaterialGrade").val());
		formData.append("ClassofSafety", $("#ClassofSafety").val());
		formData.append("ClassificationDesignation", $("#ClassificationDesignation").val());
		formData.append("Group", $("#Group").val());
		formData.append("SeismicCategory", $("#SeismicCategory").val());
		formData.append("Filename", $("#Filename").val());
		formData.append("ComponentId", $("#ComponentId").val());
		formData.append("ComponentsImage", $("#ComponentsImage").val());
		formData.append("MechanicalId", $("#MechanicalId").val());
		formData.append("fileComponents", $("#imgComponentUp")[0].files[0]);

		//Mechanical Properties
		formData.append("MechanicalTemperature", $("#MechanicalTemperature").val());
		formData.append("YoungModule", $("#YoungModule").val());
		formData.append("YieldStrength", $("#YieldStrength").val());
		formData.append("UltimateStrength", $("#UltimateStrength").val());
		formData.append("SpecificElongation", $("#SpecificElongation").val());
		formData.append("ReductionArea", $("#ReductionArea").val());
		formData.append("ImpactToughness", $("#ImpactToughness").val());
		formData.append("Hardness", $("#Hardness").val());

		//Physical Properties
		formData.append("PhysicalTemperature", $("#PhysicalTemperature").val());
		formData.append("LinearExpension", $("#LinearExpension").val());
		formData.append("HeatCapacity", $("#HeatCapacity").val());
		formData.append("ConductivityFactor", $("#ConductivityFactor").val());
		formData.append("NormalRadiation", $("#NormalRadiation").val());
		formData.append("PoissonRatio", $("#PoissonRatio").val());
		formData.append("Density", $("#Density").val());

		//Chemical Component
		formData.append("C", $("#C").val());
		formData.append("Si", $("#Si").val());
		formData.append("Mn", $("#Mn").val());
		formData.append("Cr", $("#Cr").val());
		formData.append("Ni", $("#Ni").val());
		formData.append("Mo", $("#Mo").val());
		formData.append("V", $("#V").val());
		formData.append("Ti", $("#Ti").val());
		formData.append("Cu", $("#Cu").val());
		formData.append("S", $("#S").val());
		formData.append("P", $("#P").val());
		formData.append("As", $("#As").val());
		formData.append("Co", $("#Co").val());
		formData.append("NB", $("#NB").val());

		//Heat Treatment
		formData.append("TreatmentTemperature", $("#TreatmentTemperature").val());
		formData.append("HeatsOperation", $("#HeatsOperation").val());
		formData.append("CoolingMethod", $("#CoolingMethod").val());
		formData.append("NoOfHeatOperations", $("#NoOfHeatOperations").val());
		formData.append("DocumentNo", $("#DocumentNo").val());
		formData.append("TimesOfHeating", $("#TimesOfHeating").val());

		var result = confirm("Are You Sure ?");
		if (result) {
			$.ajax({
				type: 'POST',
				url: url,
				data: formData,
				processData: false,
				contentType: false
			}).done(function () {
				$("#Tabs").load("/Technical/Components/@Model.MechanicalId");
			});
		}
	}



	function readURL(input) {

		if (input.files && input.files[0]) {
			var reader = new FileReader();

			reader.onload = function (e) {
				$('#imgCourse').attr('src', e.target.result);
			}

			reader.readAsDataURL(input.files[0]);
		}
	}

	$("#imgCourseUp").change(function () {
		readURL(this);
	});

	$('input[type=file]').change(function () {

		$('#Filename').attr('value', this.files[0].name);
	});
</script>

